pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building SCM'
            }
        }
         stage('Test') {
            steps {
                echo 'Testing SCM'
            }
        }
         stage('Approve') {
            steps {
                echo 'Approving SCM'
            }
        }
         stage('Deploy') {
            steps {
                echo 'Deploying SCM'
            }
        }
    }
}
